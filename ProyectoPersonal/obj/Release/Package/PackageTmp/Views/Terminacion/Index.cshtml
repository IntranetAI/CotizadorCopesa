            @Html.DisplayFor(modelItem => item.Componente.NombreComponente)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombreterminacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CostoFijoTerminacion)
        </td>
